{
    "version": 2,
    "name": "nextjs-cra",
    "builds": [
        { "src": "nextjs/package.json", "use": "@now/next" },
        { "src": "api/data/index.js", "use": "@now/node" }
    ],
    "routes": [
        { "src": "/api/data", "dest": "/api/data" },
        { "src": "/(.*)", "dest": "/nextjs/$1" }
    ]
}